CREATE TABLE `ggwp`.`Aluguel` ( `idaluguel` INT NOT NULL AUTO_INCREMENT , `nomejogo` VARCHAR(50) NOT NULL , `nomecliente` VARCHAR(50) NOT NULL , `diaaluguel` VARCHAR(15) NOT NULL , `devolucao` VARCHAR(15) NOT NULL , PRIMARY KEY (`idaluguel`)) ENGINE = InnoDB;

CREATE TABLE `ggwp`.`Cliente` ( `idcliente` INT NOT NULL AUTO_INCREMENT , `nomecliente` VARCHAR(50) NOT NULL , `telefone` INT(15) NOT NULL , `endereco` VARCHAR(100) NOT NULL , PRIMARY KEY (`idcliente`)) ENGINE = InnoDB;

CREATE TABLE `ggwp`.`Jogo` ( `idjogo` INT NOT NULL AUTO_INCREMENT , `nomejogo` VARCHAR(50) NOT NULL , `estilojogo` VARCHAR(20) NOT NULL , `console` VARCHAR(20) NOT NULL , `valor` DECIMAL NOT NULL , PRIMARY KEY (`idjogo`)) ENGINE = InnoDB;